{% if item.files|length >0 %}
    <div class="card-footer ">
        <ul>
            {% for file in item.files %}
                <li><a href="{{ object_url(item_file_view_route, file, {'template': 'square150'}) }}">{{ file.id.hex }}</a></li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
